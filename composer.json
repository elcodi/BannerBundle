{
    "name": "elcodi/banner-bundle",
    "description": "Elcodi Banner component",
    "keywords": ["elcodi", "banner", "ecommerce"],
    "homepage": "https://github.com/elcodi/BannerBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        },
        {
            "name": "Aldo Chiecchia",
            "email": "zimage@tiscali.it"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/elcodi/BannerBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/common": "~2.4",
        "doctrine/orm": "~2.4",
        "symfony/http-kernel": "~2.4",

        "elcodi/core-bundle": "~0.1",
        "elcodi/language-bundle": "~0.1",
        "elcodi/media-bundle": "~0.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "symfony/browser-kit": "~2.4",
        "symfony/framework-bundle": "~2.4",
        "symfony/validator": "~2.4",
        "symfony/finder": "~2.4",
        "phpunit/phpunit": ">=4.0",
        "symfony/routing": "~2.4",
        "symfony/console": "~2.4"
    },
    "autoload": {
        "psr-4": {
            "Elcodi\\BannerBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
